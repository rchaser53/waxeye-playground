{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2015", "es2016", "dom", "es2017.object"],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": true,
    "baseUrl": "",
    "outDir": "dist",
    "downlevelIteration": true
  },
  "include": ["src/index.ts"]
}